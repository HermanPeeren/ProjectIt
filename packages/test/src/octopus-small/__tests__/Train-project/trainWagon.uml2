<package> trainWagon

<class> Train
<operations> 
+ unlink(w:Wagon);
<endclass> 

<class> Wagon
<attributes> 
+ id: String;
+ bar: Boolean;
<endclass> 

<class> SleepingCar <specializes> Wagon
<attributes> 
+ numberOfBeds: Integer;
<endclass>

<associations> 
// should be: + Train.train[1] <-> + Wagon.wagon[0..*];
